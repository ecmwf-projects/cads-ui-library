{
  "name": "@ecmwf-projects/cads-ui-library",
  "version": "1.7.0",
  "description": "Common UI kit library",
  "repository": {
    "type": "git",
    "url": "git@github.com:ecmwf-projects/cads-ui-library.git"
  },
  "bugs": {
    "url": "https://github.com/ecmwf-projects/cads-ui-library/issues"
  },
  "homepage": "https://github.com/ecmwf-projects/cads-ui-library#README",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:components": "cypress open",
    "test:components:ci": "cypress run --component",
    "format": "prettier --write .",
    "lint:format": "prettier --check .",
    "lint": "eslint src/**",
    "lint:staged": "lint-staged",
    "preversion": "npm run build",
    "prepare": "husky install",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^2.2.0",
    "cypress": "^12.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^7.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "type-fest": "^2.19.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.0.0",
    "@radix-ui/react-checkbox": "^0.1.5",
    "@radix-ui/react-icons": "^1.1.1",
    "@radix-ui/react-label": "^0.1.5",
    "@radix-ui/react-radio-group": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "markdown-to-jsx": "^7.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  }
